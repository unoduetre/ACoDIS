<?xml version='1.1' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>pl.lodz.p.pracowniaproblemowa.acodis.resources.messages</base-name>
            <var>msgs</var>
        </resource-bundle>
        <resource-library-contracts>
            <contract-mapping>
                <url-pattern>*</url-pattern>
                <contracts>template</contracts>
            </contract-mapping>
        </resource-library-contracts>
        <locale-config>
            <default-locale>pl</default-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
    </application>
    <!-- view request session application none -->
    <managed-bean>
        <managed-bean-name>prolog</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.prolog.Prolog</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
        <managed-property>
            <property-name>libPath</property-name>
            <property-class>java.lang.String</property-class>
            <value>/pl/lodz/p/pracowniaproblemowa/acodis/resources/lib.pl</value>
        </managed-property>
        <managed-property>
            <property-name>accessPath</property-name>
            <property-class>java.lang.String</property-class>
            <value>/pl/lodz/p/pracowniaproblemowa/acodis/resources/access.pl</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>protect</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.protect.Protect</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
        <managed-property>
            <property-name>prolog</property-name>
            <property-class>pl.lodz.p.pracowniaproblemowa.acodis.prolog.Prolog</property-class>
            <value>#{prolog}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>security</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.security.Security</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>prolog</property-name>
            <property-class>pl.lodz.p.pracowniaproblemowa.acodis.prolog.Prolog</property-class>
            <value>#{prolog}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.login.Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>filebrowserbean</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.filebrowser.FileBrowserBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>wikiEditorBean</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.wiki.WikiEditorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>installTestDataBean</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.wiki.InstallTestDataBean</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>wikiReaderBean</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.wiki.WikiReaderBean</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>wikiMenuBean</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.wiki.WikiMenuBean</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>wikiCategoryManagerBean</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.wiki.WikiCategoryManagerBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>profile</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.profile.Profile</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>profileDataBean</managed-bean-name>
        <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.profile.ProfileDataBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
        <managed-property>
            <property-name>login</property-name>
            <property-class>pl.lodz.p.pracowniaproblemowa.acodis.login.Login</property-class>
            <value>#{login}</value>
        </managed-property>
    </managed-bean>
</faces-config>
