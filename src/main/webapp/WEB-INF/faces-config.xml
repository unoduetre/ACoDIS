<?xml version='1.1' encoding='UTF-8'?>
<faces-config version="2.2"
  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <application>
    <resource-bundle>
      <base-name>messages</base-name>
      <var>msgs</var>
    </resource-bundle>
    <resource-library-contracts>
      <contract-mapping>
        <url-pattern>*</url-pattern>
        <contracts>template</contracts>
      </contract-mapping>
    </resource-library-contracts>
    <locale-config>
      <default-locale>pl</default-locale>
      <supported-locale>en</supported-locale>
    </locale-config>
  </application>
  <!-- view request session application none -->
  <managed-bean>
    <managed-bean-name>prolog</managed-bean-name>
    <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.prolog.Prolog</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>protect</managed-bean-name>
    <managed-bean-class>pl.lodz.p.pracowniaproblemowa.acodis.protect.Protect</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</faces-config>
